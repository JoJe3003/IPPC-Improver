{
    "name": "IPPC Improver",
    "description": "Visual improvements to IPPC.no, for reading METAR and TAF.",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {  "16": "icon16.png",
                "32": "icon32.png",
                "48": "icon48.png",
                "64": "icon64.png",
                "128": "icon128.png" },

    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["script.js"],
        "matches": ["https://www.ippc.no/ippc/*"]
    }]
}